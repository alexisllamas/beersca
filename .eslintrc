{
  "extends": "react-app",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all"
    }],
    "radix": 0,
    "no-unused-vars": ["error", {
      "args": "after-used"
    }],
    "no-console": "error",
    "no-var": "error"
  }
}
